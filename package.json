{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "check:emulators": "node scripts/check-emulators.js",
    "check:firebase-config": "node scripts/check-firebase-config.js",
    "copy-assets:unix": "mkdir -p client/public/images && cp -r public/images/* client/public/images/ && cp public/*.png public/*.ico public/*.svg public/site.webmanifest client/public/",
    "copy-assets:win": "if not exist client\\public\\images mkdir client\\public\\images && xcopy /E /Y public\\images client\\public\\images\\ && copy /Y public\\*.png public\\*.ico public\\*.svg public\\site.webmanifest client\\public\\",
    "copy-assets": "node -e \"process.platform === 'win32' ? process.exit(require('child_process').spawnSync('npm', ['run', 'copy-assets:win'], {stdio:'inherit'}).status) : process.exit(require('child_process').spawnSync('npm', ['run', 'copy-assets:unix'], {stdio:'inherit'}).status)\"",
    "dev:client": "kill-port 5173 && vite --force",
    "dev:server": "kill-port 3001 && tsx --watch server/server/index.ts",
    "dev:original": "npm run copy-assets && concurrently --kill-others-on-fail \"npm run dev:server\" \"node -e \\\"setTimeout(() => require('child_process').spawn('npm', ['run', 'dev:client'], {stdio:'inherit'}), 2000)\\\"\"",
    "dev": "npm run copy-assets && concurrently --kill-others-on-fail \"npm run dev:server\" \"node -e \\\"setTimeout(() => require('child_process').spawn('npm', ['run', 'dev:client'], {stdio:'inherit'}), 2000)\\\"\"",
    "dev:with-checks": "npm run copy-assets && npm run check:emulators && concurrently --kill-others-on-fail \"npm run dev:server\" \"node -e \\\"setTimeout(() => require('child_process').spawn('npm', ['run', 'dev:client'], {stdio:'inherit'}), 2000)\\\"\"",
    "build:client": "cd client && npm install react-helmet-async && cd .. && vite build",
    "build:server": "esbuild server/server/index.ts --platform=node --bundle --format=esm --outfile=dist/index.js --packages=external --target=es2022",
    "build": "node scripts/optimize-build.js && npm run copy-assets && npm run build:client && npm run build:server",
    "build:analyze": "cd client && npm install react-helmet-async && cd .. && vite build --mode analyze",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "start": "cross-env NODE_ENV=production tsx server/server/index.ts",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "db:migrate": "tsx scripts/run-migrations.ts",
    "db:migrate:test": "cross-env NODE_ENV=test tsx scripts/run-migrations.ts",
    "db:generate": "tsx scripts/generate-migrations.ts",
    "db:studio": "drizzle-kit studio",
    "db:status": "drizzle-kit status",
    "create:test-users": "tsx scripts/create-test-users.js",
    "test:auth-persistence": "tsx scripts/test-auth-persistence.js",
    "test:email-link": "tsx scripts/test-email-link-auth.js",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:server": "vitest run server/**/*.test.{ts,js}",
    "test:server:coverage": "vitest run server/**/*.test.{ts,js} --coverage",
    "test:integration": "vitest run server/tests/integration/**/*.test.{ts,js}",
    "test:unit": "vitest run server/tests/unit/**/*.test.{ts,js}",
    "test:api": "vitest run tests/api/**/*.test.{ts,js}",
    "test:performance": "tsx tests/performance/api-performance.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:visual": "playwright test tests/visual/*.spec.ts",
    "test:a11y": "playwright test tests/accessibility/*.spec.ts",
    "test:smoke:staging": "playwright test tests/smoke/ --config=playwright.staging.config.ts",
    "test:smoke:production": "playwright test tests/smoke/ --config=playwright.production.config.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "lighthouse": "lhci autorun",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "netlify:dev": "netlify dev",
    "netlify:build": "npm run copy-assets && npm run build:client:optimized && npm run netlify:post-build",
    "build:client:optimized": "cd client && npm install react-helmet-async && cd .. && vite build --mode production --minify terser",
    "netlify:prepare": "node scripts/prepare-netlify-deploy.js",
    "netlify:validate": "node scripts/validate-netlify-config.js",
    "netlify:test": "node scripts/test-deployment.js",
    "netlify:security": "node scripts/validate-security.js",
    "firebase:validate": "node scripts/validate-firebase.js",
    "deploy:fast": "node scripts/fast-deploy.js",
    "netlify:post-build": "node scripts/netlify-post-build.js",
    "deploy:prod": "npm run build && netlify deploy --prod",
    "firebase:build": "npm run build",
    "firebase:deploy": "firebase deploy --only hosting",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "firebase:deploy:all": "firebase deploy",
    "firebase:deploy:rules": "firebase deploy --only firestore:rules,storage:rules",
    "deploy:firebase": "node scripts/firebase-deploy.js",
    "deploy:firebase:all": "node scripts/firebase-deploy.js all",
    "deploy:firebase:functions": "node scripts/firebase-deploy.js functions",
    "firebase:serve": "firebase serve --only hosting",
    "firebase:emulators": "firebase emulators:start",
    "env:sanitize": "node scripts/sanitize-env.js",
    "env:check": "node -e \"import('fs').then(fs => { if(fs.existsSync('.env') && fs.existsSync('client/.env')) { console.log('✅ Environment files exist'); process.exit(0); } else { console.error('❌ Missing .env files. Run: cp .env.example .env && cp .env.example client/.env'); process.exit(1); } })\"",
    "env:setup-prod": "node scripts/setup-production-env.js set",
    "env:check-prod": "node scripts/setup-production-env.js check",
    "setup:ssh": "./scripts/setup-github-ssh.sh",
    "verify:ssh": "./scripts/verify-ssh-github.sh",
    "qa:all": "npm run lint && npm run type-check && npm run test:coverage && npm run test:e2e",
    "qa:quick": "npm run lint && npm run type-check && npm run test",
    "precommit": "npm run qa:quick"
  },
